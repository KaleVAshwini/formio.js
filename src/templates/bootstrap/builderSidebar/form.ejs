<div id="{{ctx.groupId}}" class="accordion builder-sidebar{{ctx.scrollEnabled ? ' builder-sidebar_scroll' : ''}}" ref="sidebar">
  <!-- <input class="form-control builder-sidebar_search" type="search" ref="sidebar-search" placeholder="Search field(s)" /> -->
 
  <div class="container " ref="sidebar-groups">
    <ul class="nav nav-tabs" id="myTab" role="tablist">
      <li class="nav-item" role="presentation">
        <a class="nav-link active" onclick="event.stopPropagation()" id="home-tab" data-toggle="tab" target="_self" href="#basic" role="tab" aria-controls="basic" aria-selected="true">Basic</a >
      </li>
      <li class="nav-item" role="presentation">
        <a onclick="event.stopPropagation()" class="nav-link" id="profile-tab" data-toggle="tab" target="_self" href="#custom" role="tab" aria-controls="custom" aria-selected="false">Custom</a>
        </li>
      <li class="nav-item" role="presentation">
        <a  class="nav-link" id="contact-tab" data-toggle="tab" target="_self" href="#others" role="tab" aria-controls="others" aria-selected="false">Others</a>
        </li>
    </ul>
    <div class="tab-content" id="myTabContent">
      <div class="tab-pane fade show active" id="basic"  role="tabpanel" aria-labelledby="basic-tab">  {% ctx.groups.forEach(function(group) { %}
        {{ group }}
      {% }) %}</div>
      <div class="tab-pane fade" id="custom" role="tabpanel" aria-labelledby="custom-tab">  {% ctx.groups.forEach(function(group) { %}
        {{ group }}
      {% }) %}</div>
      <div class="tab-pane fade" id="others" role="tabpanel" aria-labelledby="others-tab"> asdas</div>
    </div>
    
  </div>
</div>
